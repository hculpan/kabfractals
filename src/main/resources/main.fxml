<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vbox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.culpan.kabfractals.MainController">
   <children>
      <ButtonBar prefHeight="40.0" prefWidth="200.0">
        <buttons>
            <HBox fx:id="configHBox" prefHeight="40.0" prefWidth="1031.0" />
            <ChoiceBox fx:id="fractalChoiceBox" maxWidth="400.0" prefHeight="25.0" prefWidth="150.0" />
          <Button fx:id="generateButton" maxWidth="400.0" mnemonicParsing="false" onAction="#generateAction" text="Generate" />
        </buttons>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </ButtonBar>
      <Canvas fx:id="canvas" height="600.0" width="1200.0" />
   </children>
</VBox>
